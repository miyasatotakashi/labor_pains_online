<h1>承認確認</h1>
<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>


  <tbody>
    <% @app_users.each do |user| %>
      <tr>
     
        <td><%= link_to "承認", allow_partners_path(partners_id: user.app_id), method: :post %></td>
        <td><%= link_to "拒否", allow_partners_path(partners_id: user.app_id), method: :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'パートナー登録へ戻る', partners_path %>